{
  "name": "h4-server",
  "version": "0.0.0",
  "license": "MIT",
  "description": "h4-server.",
  "main": "dist/index.js",
  "typings": "src/index.ts",
  "scripts": {
    "lint": "tslint ./src/**/*.ts ./src/**/*.test.ts ./src/**/*.d.ts",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "js": "rimraf dist-esnext && npm run tsc && npm run babel",
    "babel":" rimraf dist && babel dist-esnext -d dist --presets es2015 --source-maps",
    "babel:w":" rimraf dist && babel dist-esnext --watch -d dist --presets es2015 --source-maps",
    "pretest": "npm-run-all --parallel js lint",
    "mocha": "mocha ./dist/**/*.test.js --require babel-polyfill",
    "test": "nyc npm run mocha && nyc report --reporter=html --reporter=text-lcov > coverage.lcov && f() { EXIT=0; codecov --token=1d2b8efd-fdb1-40aa-8068-e6b159eed362 || exit $EXIT; }; f",
    "test:cover": "npm run test && opn ./coverage/index.html",
    "predebug": "npm run pretest",
    "debug": "node --nolazy --debug-brk=5858 dist/index.js",
    "docs:build": "typedoc --out ./docs ./ --includeDeclarations --excludeExternals --exclude '**/*.test.ts' 'node_modules'",
    "docs": "npm run docs:build && opn ./docs/index.html",
    "start": "npm-run-all --parallel gensalt server",
    "start:dev": "nodemon dist/index --exec babel-node --presets es2015",
    "server": "npm run js && babel-node --presets es2015 dist/index.js",
    "gensalt": "node gensalt.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alanmarcell/h4-server.git"
  },
  "author": "github.com/alanmarcell",
  "bugs": {
    "url": "https://github.com/alanmarcell/h4-server/issues"
  },
  "homepage": "https://github.com/alanmarcell/h4-server#readme",
  "devDependencies": {
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.12",
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "codecov": "^2.1.0",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^10.2.0",
    "opn-cli": "^3.1.0",
    "ptz-assert": "^1.6.3",
    "rimraf": "^2.6.1",
    "tslint": "^5.0.0",
    "typedoc": "^0.6.0",
    "typescript": "^2.5.2"
  },
  "dependencies": {
    "@types/express": "^4.0.36",
    "@types/mongodb": "^2.2.11",
    "@types/mongoose": "^4.7.18",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "chai": "^4.0.2",
    "cors": "^2.8.3",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "jsonwebtoken": "^7.4.1",
    "mongodb": "^2.2.25",
    "mongoose": "^4.10.7",
    "morgan": "^1.8.2",
    "ptz-log-file": "^1.0.0",
    "ramda": "^0.24.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "es2017"
    ]
  }
}
